    <form>
      <div class="form-group">
        <label for="jobName">Job Name</label>
        <textarea class="form-control" id="jobName" rows="1" placeholder="eg. Replaced Break Pads"></textarea>
      </div>
      <div class="form-group">
        <label for="jobDescription">Job Description</label>
        <textarea class="form-control" id="jobDescription" rows="3"></textarea>
      </div>
      <div class="form-group">
        <label for="milage">Milage</label>
        <textarea class="form-control" id="milage" rows="1"></textarea>
      </div>
      <div class="form-group">
        <label for="parts">Parts</label>
        <textarea class="form-control" id="parts" rows="1"></textarea>
      </div>
      <div class="form-group">
        <label for="vehicle">Vehicle</label>
        <select class="form-control" id="vehicle"></select>
      </div>
      <div class="form-group">
        <label for="date">Job Date</label>
        <textarea class="form-control" id="date" rows="1"></textarea>
      </div>
      <button type="submit" class="btn btn-primary" id="submit">Submit</button>
    </form>

